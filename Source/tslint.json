{
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [ true ],
		"triple-equals": true,
		"no-var-keyword": true,
		"no-bitwise": true,
		"no-unexternalized-strings": [
			true,
			{
				"signatures": ["localize", "nls.localize"],
				"keyIndex": 0,
				"messageIndex": 1
			}
		]
	},
	"defaultSeverity": "warning"
}