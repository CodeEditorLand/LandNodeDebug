			{
				"category": "Debug",
				"command": "extension.node-debug.toggleSkippingFile",
				"title": "%toggle.skipping.this.file%"
			}
		],
		"configuration": {
			"properties": {
				"debug.node.showUseWslIsDeprecatedWarning": {
					"default": true,
					"description": "%debug.node.showUseWslIsDeprecatedWarning.description%",
					"scope": "window",
					"type": "boolean"
				}
			},
			"title": "Node debug"
		},
