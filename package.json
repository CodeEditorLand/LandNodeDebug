		"breakpoints": [
			{
				"language": "javascript"
			},
			{
				"language": "javascriptreact"
			}
		],
		"commands": [
			{
				"category": "Debug",
				"command": "extension.node-debug.attachNodeProcess",
				"title": "%attach.node.process%"
